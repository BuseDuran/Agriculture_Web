@model EntityLayer.Concrete.Contact

@{
    ViewData["Title"] = "EditTeam";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Message Details</h4>
            <form class="forms-sample" method="post">
                @Html.HiddenFor(x=>x.ContactID)
                <div class="form-group">
                    <label class="control-label">Name and Surname</label>
                    <input disabled type="text" class="form-control" asp-for="Name">
                </div>
                 <div class="form-group">
                    <label class="control-label">Date</label>
                    <input disabled type="datetime" class="form-control" asp-for="Date">
                </div>
                <div class="form-group">
                    <label class="control-label">Message Detail</label>
                    <textarea disabled class="form-control"  cols="5" rows="20" asp-for="Message"></textarea>
                </div>

            </form>
        </div>
    </div>
</div>