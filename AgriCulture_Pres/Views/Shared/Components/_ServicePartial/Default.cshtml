@using EntityLayer.Concrete
@model List<Service>

<div class="sevices-w3layouts" id="services">
	<div class="container">
		<h3 class="tittle">Our Services</h3>
		@foreach(var item in Model)
			{
		<div class="services-main">
			<div class="@item.Class1 " style="color:azure" data-wow-delay=".5s">
				<i class="@item.Image" aria-hidden="true"></i>
				<h5>@item.Title</h5>					<p>@item.Description</p>
				<div class="@item.Class2"></div>
			</div>
			
		</div>
		}
		<div class="clearfix"> </div>
	</div>
</div>